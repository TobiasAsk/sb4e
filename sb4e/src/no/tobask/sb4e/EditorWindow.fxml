<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane dividerPositions="0.2, 0.8" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8.0.151" xmlns:fx="http://javafx.com/fxml/1">
  <items>
      <SplitPane fx:id="libraryDocumentSplitPane" dividerPositions="0.5" orientation="VERTICAL" maxWidth="450.0" minWidth="280.0" SplitPane.resizableWithParent="false">
        <items>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox alignment="CENTER" spacing="0.0">
                     <children>
                        <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="Library" HBox.hgrow="NEVER" />
                        <StackPane fx:id="librarySearchPanelHost" maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="" HBox.hgrow="ALWAYS" />
                        <MenuButton fx:id="libraryMenuButton" mnemonicParsing="false" HBox.hgrow="NEVER">
                          <items>
                            <MenuItem mnemonicParsing="false" text="Action 1" />
                            <MenuItem mnemonicParsing="false" text="Action 2" />
                          </items>
                        </MenuButton>
                     </children></HBox>
                  <StackPane fx:id="libraryPanelHost" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox alignment="CENTER" spacing="0.0" VBox.vgrow="NEVER">
                  	<children>
						<Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="Document" HBox.hgrow="NEVER" />
						<Pane maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
						<MenuButton fx:id="" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="" HBox.hgrow="NEVER">
							<items>
								<Menu mnemonicParsing="false" text="%hierarchy.displays">
									<items>
										<RadioMenuItem fx:id="showInfoMenuItem"
											mnemonicParsing="false" onAction="#onHierarchyShowInfo" selected="true"
											text="%hierarchy.displayoption.info">
											<toggleGroup>
												<ToggleGroup fx:id="hierarchyDisplayOptionTG" />
											</toggleGroup>
										</RadioMenuItem>
										<RadioMenuItem fx:id="showFxIdMenuItem"
											mnemonicParsing="false" onAction="#onHierarchyShowFxId" text="%hierarchy.displayoption.fxid"
											toggleGroup="$hierarchyDisplayOptionTG" />
										<RadioMenuItem fx:id="showNodeIdMenuItem"
											mnemonicParsing="false" onAction="#onHierarchyShowNodeId" text="%hierarchy.displayoption.nodeid"
											toggleGroup="$hierarchyDisplayOptionTG" />
									</items>
								</Menu>
							</items>
							<graphic>
								<Region prefHeight="10.0" prefWidth="10.0" styleClass="cog-shape" />
							</graphic>
						</MenuButton>
                  	</children>
                  </HBox>
                  <Accordion maxHeight="-1.0" VBox.vgrow="ALWAYS">
                    <panes>
                      <TitledPane animated="true" text="%hierarchy">
                           <content>
                            <StackPane fx:id="hierarchyPanelHost" maxHeight="-1.0" style="-fx-padding: 0;" />
                           </content>
                      </TitledPane>
                      <TitledPane animated="true" text="%controller">
                           <content>
                           	<StackPane fx:id="infoPanelHost" maxHeight="-1.0" style="-fx-padding: 0;" />
                           </content>
                      </TitledPane>
                    </panes>
                  </Accordion>
               </children>
            </VBox>
        </items>
      </SplitPane>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <center>
            <StackPane fx:id="contentPanelHost" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <StackPane fx:id="selectionBarHost" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </top></BorderPane>
  </items>
</SplitPane>
