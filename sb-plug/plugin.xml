<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="sbplug.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="notexists"
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="sbplug.editors.XMLEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="xml"
            icon="icons/sample.png"
            id="sbplug.editors.XMLEditor"
            name="Sample XML Editor">
      </editor>
      <editor
            class="sbplug.editors.FXMLEditor"
            contributorClass="sbplug.editors.FXMLEditorContributor"
            default="false"
            extensions="fxml"
            id="sbplug.editors.FXMLEditor"
            name="SB FXML Editor">
      </editor>
   </extension>

</plugin>
